{
  "name": "sales-applet",
  "version": "1.0.0",
  "private": true,
  "description": "taro",
  "type": "module",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "Sass",
    "framework": "Vue3"
  },
  "scripts": {
    "prepare": "husky",
    "mock-server": "pnpm -F mocks run server",
    "dev": "taro build --type weapp --watch",
    "dev:mock": "run-p mock-server dev:local",
    "dev:local": "cross-env IS_MOCK=true API_HOST=localhost:3003 HTTPS=false taro build --type weapp --watch",
    "build": "taro build --type weapp",
    "build:preview": "cross-env API_HOST=gateway.upfreework.com taro build --type weapp",
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:harmony-hybrid": "taro build --type harmony-hybrid",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:harmony-hybrid": "npm run build:harmony-hybrid -- --watch",
    "lint": "run-p oxc eslint stylelint",
    "lint:fix": "run-p oxc:fix eslint:fix stylelint:fix",
    "oxc": "oxlint",
    "oxc:fix": "oxlint --fix",
    "eslint": "eslint \"**/*.{ts,tsx,vue}\"",
    "eslint:fix": "eslint \"**/*.{ts,tsx,vue}\" --fix",
    "stylelint": "stylelint \"src/**/*.{css,less,scss,vue}\"",
    "stylelint:fix": "stylelint \"src/**/*.{css,less,scss,vue}\" --fix"
  },
  "browserslist": {
    "development": [
      "defaults and fully supports es6-module",
      "maintained node versions"
    ],
    "production": [
      "last 3 versions",
      "Android >= 4.1",
      "ios >= 8"
    ]
  },
  "author": "",
  "dependencies": {
    "@babel/runtime": "^7.28.4",
    "@nutui/icons-vue-taro": "^0.0.9",
    "@nutui/nutui-taro": "^4.3.14",
    "@tarojs/components": "4.1.1",
    "@tarojs/helper": "4.1.1",
    "@tarojs/plugin-framework-vue3": "4.1.1",
    "@tarojs/plugin-html": "4.1.1",
    "@tarojs/plugin-platform-alipay": "4.1.1",
    "@tarojs/plugin-platform-h5": "4.1.1",
    "@tarojs/plugin-platform-harmony-hybrid": "4.1.1",
    "@tarojs/plugin-platform-jd": "4.1.1",
    "@tarojs/plugin-platform-qq": "4.1.1",
    "@tarojs/plugin-platform-swan": "4.1.1",
    "@tarojs/plugin-platform-tt": "4.1.1",
    "@tarojs/plugin-platform-weapp": "4.1.1",
    "@tarojs/runtime": "4.1.1",
    "@tarojs/shared": "4.1.1",
    "@tarojs/taro": "4.1.1",
    "qs": "^6.14.0",
    "vue": "^3.5.26"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-transform-class-properties": "7.25.9",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@nutui/auto-import-resolver": "^1.0.0",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@tarojs/cli": "4.1.1",
    "@tarojs/taro-loader": "4.1.1",
    "@tarojs/test-utils-vue3": "^0.1.1",
    "@tarojs/webpack5-runner": "4.1.1",
    "@types/node": "^24.10.4",
    "@types/webpack-env": "^1.18.8",
    "@typescript-eslint/parser": "^8.50.0",
    "@vue/babel-plugin-jsx": "^2.0.1",
    "@vue/compiler-sfc": "^3.5.26",
    "babel-preset-taro": "4.1.1",
    "cross-env": "^10.1.0",
    "css-loader": "^7.1.2",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^10.6.2",
    "husky": "^9.1.7",
    "npm-run-all": "^4.1.5",
    "oxlint": "^1.34.0",
    "postcss": "^8.5.6",
    "postcss-scss": "^4.0.9",
    "rimraf": "^6.1.2",
    "sass": "^1.97.0",
    "style-loader": "^4.0.0",
    "stylelint": "^16.26.1",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard": "^39.0.1",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^5.9.3",
    "unplugin-vue-components": "^29.2.0",
    "vue-eslint-parser": "^10.2.0",
    "vue-loader": "^17.4.2",
    "vue-tsc": "^3.1.8",
    "webpack": "5.91.0"
  }
}
